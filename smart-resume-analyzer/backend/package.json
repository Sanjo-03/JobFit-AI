{
  "type": "module",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon --watch \"./**/*.js\" --exec \"node --import dotenv/config --experimental-specifier-resolution=node\" server.js",
    "dev:node": "node --import ts-node/register server.ts",
    "build": "tsc",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "openai": "^5.9.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.5.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.3",
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.1"
  }
}